# mount point     fstype    device           device2                    flags

/system           ext4      /dev/block/bootdevice/by-name/system        flags=display="System";fsflags="discard";backup=1;wipeingui
/system_image     emmc      /dev/block/bootdevice/by-name/system        flags=display="System";backup=1;flashimg=1
/vendor           ext4      /dev/block/bootdevice/by-name/vendor        flags=display="Vendor";fsflags="discard";backup=1;wipeingui
/vendor_image     emmc      /dev/block/bootdevice/by-name/vendor        flags=display="Vendor";backup=1;flashimg=1
